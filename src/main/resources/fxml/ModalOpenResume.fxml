<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<StackPane fx:id="modalPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.iworking.personnel.reserve.controller.ModalOpenResumeFxmlController">
   <StackPane fx:id="backgroundPane" prefWidth="460.0">
      <children>
         <GridPane maxHeight="1.7976931348623157E308">
            <columnConstraints>
               <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" />
               <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" />
            </columnConstraints>
            <rowConstraints>
               <RowConstraints vgrow="SOMETIMES" />
               <RowConstraints vgrow="SOMETIMES" />
               <RowConstraints vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
               <VBox spacing="5.0" GridPane.columnIndex="1">
                  <children>
                     <Separator prefWidth="200.0" />
                     <Label text="Ф.И.О.:">
                        <font>
                           <Font size="10.0" />
                        </font>
                     </Label>
                     <Label fx:id="lastNameLabel" text="Фамилия" />
                     <Label fx:id="firstNameLabel" text="Имя" />
                     <Label fx:id="middleNameLabel" text="Отчество" />
                     <Separator prefWidth="200.0" />
                     <VBox>
                        <children>
                           <Label text="Тел.:">
                              <font>
                                 <Font size="10.0" />
                              </font>
                           </Label>
                           <Label fx:id="numberPhoneLabel" text="+7 999 999 99 99" />
                        </children>
                     </VBox>
                     <VBox>
                        <children>
                           <Label text="Эл. почта:">
                              <font>
                                 <Font size="10.0" />
                              </font>
                           </Label>
                           <Label fx:id="emailLabel" text="test@mail.ru" />
                        </children>
                     </VBox>
                  </children>
                  <padding>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </padding>
               </VBox>
               <VBox spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                  <children>
                     <Separator prefWidth="200.0" />
                     <VBox>
                        <children>
                           <Label text="Профессия:">
                              <font>
                                 <Font size="10.0" />
                              </font>
                           </Label>
                           <Label fx:id="professionLabel" text="Профессия" />
                        </children>
                     </VBox>
                     <VBox>
                        <children>
                           <Label text="Профобласть:">
                              <font>
                                 <Font size="10.0" />
                              </font>
                           </Label>
                           <Label fx:id="profFieldLabel" text="Профобласть" />
                        </children>
                     </VBox>
                     <VBox>
                        <children>
                           <Label text="Зарплата:">
                              <font>
                                 <Font size="10.0" />
                              </font>
                           </Label>
                           <Label fx:id="wageLabel" text="99 999" />
                        </children>
                     </VBox>
                     <VBox>
                        <children>
                           <Label text="График:">
                              <font>
                                 <Font size="10.0" />
                              </font>
                           </Label>
                           <Label fx:id="workTypeLabel" text="Полный день" />
                        </children>
                     </VBox>
                  </children>
                  <padding>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </padding>
               </VBox>
               <VBox spacing="5.0" GridPane.rowIndex="1">
                  <children>
                     <Separator prefWidth="200.0" />
                     <VBox>
                        <children>
                           <Label text="Образование:">
                              <font>
                                 <Font size="10.0" />
                              </font>
                           </Label>
                           <Label fx:id="educationLabel" text="Среднее" />
                        </children>
                     </VBox>
                     <VBox>
                        <children>
                           <Label text="Опыт работы:">
                              <font>
                                 <Font size="10.0" />
                              </font>
                           </Label>
                           <Label fx:id="experienceLabel" text="От 1 года" />
                        </children>
                     </VBox>
                     <VBox>
                        <children>
                           <Label text="Адрес:">
                              <font>
                                 <Font size="10.0" />
                              </font>
                           </Label>
                        </children>
                     </VBox>
                     <Label fx:id="addressLabel" text="Россия ....." />
                  </children>
                  <padding>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </padding>
               </VBox>
               <VBox spacing="5.0" GridPane.rowIndex="2">
                  <padding>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </padding>
                  <children>
                     <Separator prefWidth="200.0" />
                     <Button fx:id="buttonCancel" styleClass="grey" mnemonicParsing="false" onAction="#actionButtonCancel" text="Назад" />
                  </children>
               </VBox>
               <VBox GridPane.columnIndex="1" GridPane.rowIndex="2">
                  <children>
                     <Separator prefWidth="200.0" />
                  </children>
                  <padding>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </padding>
               </VBox>
            </children>
         </GridPane>
      </children>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
   </StackPane>
</StackPane>
