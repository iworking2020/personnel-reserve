<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.*?>
<fx:root fx:id="parent" spacing="10.0" type="VBox" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <HBox spacing="5.0">
         <children>
            <DatePicker fx:id="dateStartDatePicker" maxWidth="150.0" HBox.hgrow="ALWAYS" />
            <DatePicker fx:id="dateEndDatePicker" maxWidth="150.0" HBox.hgrow="ALWAYS" />
            <AnchorPane HBox.hgrow="ALWAYS" />
             <Button maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#actionDelete" prefWidth="100.0" styleClass="grey" text="Удалить" HBox.hgrow="NEVER" />
         </children>
      </HBox>
      <TextArea fx:id="editableText" prefHeight="100.0" promptText="Описание" />
    </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</fx:root>
