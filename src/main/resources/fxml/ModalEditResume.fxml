<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<StackPane fx:id="modalPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.iworking.personnel.reserve.controller.ModalEditResumeFxmlController">
   <StackPane fx:id="backgroundPane" prefWidth="460.0">
      <children>
         <GridPane maxHeight="1.7976931348623157E308">
            <columnConstraints>
               <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" />
               <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" />
            </columnConstraints>
            <rowConstraints>
               <RowConstraints vgrow="SOMETIMES" />
               <RowConstraints vgrow="SOMETIMES" />
               <RowConstraints vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
               <VBox spacing="5.0" GridPane.columnIndex="1">
                  <children>
                     <TextField fx:id="lastNameTextField" promptText="Фамилия">
                        <VBox.margin>
                           <Insets />
                        </VBox.margin>
                     </TextField>
                     <TextField fx:id="firstNameTextField" promptText="Имя">
                        <VBox.margin>
                           <Insets />
                        </VBox.margin>
                     </TextField>
                     <TextField fx:id="middleNameTextField" promptText="Отчество">
                        <VBox.margin>
                           <Insets />
                        </VBox.margin>
                     </TextField>
                     <TextField fx:id="numberPhoneTextField" promptText="Номер тел." />
                     <TextField fx:id="emailTextField" promptText="Эл. почта" />
                  </children>
                  <padding>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </padding>
               </VBox>
               <VBox spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                  <children>
                     <TextField fx:id="wageTextField" promptText="Зарплата" />
                     <ComboBox fx:id="currencyComboBox" maxWidth="1.7976931348623157E308" promptText="Валюта" />
                     <TextField fx:id="professionTextField" promptText="Профессия" />
                     <ComboBox fx:id="profFieldComboBox" maxWidth="1.7976931348623157E308" promptText="Профобласть" />
                     <ComboBox fx:id="workTypeComboBox" maxWidth="1.7976931348623157E308" promptText="График" />
                  </children>
                  <padding>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </padding>
               </VBox>
               <VBox spacing="5.0" GridPane.rowIndex="1">
                  <children>
                     <ComboBox fx:id="educationComboBox" maxWidth="1.7976931348623157E308" promptText="Образование" />
                     <DatePicker fx:id="experienceDateStartDatePicker" maxWidth="1.7976931348623157E308" promptText="Опыт работы от" />
                     <DatePicker fx:id="experienceDateEndDatePicker" maxWidth="1.7976931348623157E308" promptText="Опыт работы до" />
                     <TextArea fx:id="addressTextArea" prefHeight="57.0" prefWidth="200.0" promptText="Адрес" />
                  </children>
                  <padding>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </padding>
               </VBox>
               <VBox alignment="CENTER_RIGHT" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                  <children>
                     <Button mnemonicParsing="false" onAction="#actionButtonEdit" styleClass="grey" text="Изменить">
                        <VBox.margin>
                           <Insets />
                        </VBox.margin>
                     </Button>
                  </children>
                  <GridPane.margin>
                     <Insets />
                  </GridPane.margin>
                  <padding>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </padding>
               </VBox>
               <VBox spacing="5.0" GridPane.rowIndex="2">
                  <padding>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </padding>
                  <children>
                     <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#actionButtonCancel" styleClass="grey" text="Отмена" />
                  </children>
               </VBox>
            </children>
         </GridPane>
      </children>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
   </StackPane>
</StackPane>
