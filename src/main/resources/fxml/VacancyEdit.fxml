<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox fx:id="vacancyEdit" spacing="5.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.iworking.personnel.reserve.controller.VacancyEditController">
    <children>
        <TextField fx:id="vacancyProfessionTextField" promptText="Профессия" />
        <ComboBox fx:id="vacancyProfFieldComboBox" maxWidth="1.7976931348623157E308" promptText="Проф. область" />
        <ComboBox fx:id="vacancyWorkTypeComboBox" maxWidth="1.7976931348623157E308" prefWidth="150.0" promptText="График работы" />
        <ComboBox fx:id="vacancyEducationComboBox" maxWidth="1.7976931348623157E308" prefWidth="150.0" promptText="Образование" />
        <TextField fx:id="vacancyWageTextField" promptText="Зарплата" />
        <ComboBox fx:id="vacancyCurrencyComboBox" maxWidth="1.7976931348623157E308" prefWidth="150.0" promptText="Валюта" />
        <DatePicker fx:id="vacancyExpDateStartDatePicker" maxWidth="1.7976931348623157E308" promptText="начало опыта раб." />
        <DatePicker fx:id="vacancyExpDateEndDatePicker" maxWidth="1.7976931348623157E308" promptText="конец опыта раб." />
        <TextArea fx:id="vacancyAddressTextArea" maxHeight="-Infinity" prefHeight="25.0" promptText="Адрес" />
        <AnchorPane>
            <children>
                <Button mnemonicParsing="false" onAction="#actionCancel" styleClass="grey" text="Отмена" AnchorPane.leftAnchor="0.0" />
                <Button fx:id="saveVacancyButton" mnemonicParsing="false" styleClass="grey" text="Сохранить" visible="false" AnchorPane.rightAnchor="0.0" />
                <Button fx:id="updateVacancyButton" mnemonicParsing="false" styleClass="grey" text="Обновить" visible="false" AnchorPane.rightAnchor="0.0" />
            </children>
        </AnchorPane>
    </children>
</VBox>
