<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<StackPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.iworking.personnel.reserve.controller.MainMenuFxmlController">
    <children>
      <VBox maxHeight="1.7976931348623157E308">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="Окна">
                     <items>
                        <CheckMenuItem fx:id="winSearchCheckItem" mnemonicParsing="false" text="Окно поиска" />
                     </items>
                </Menu>
                  <Menu mnemonicParsing="false" text="Данные">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#actionLoadData" text="Загрузить" />
                      <MenuItem mnemonicParsing="false" onAction="#actionUploadData" text="Выгрузить" />
                    </items>
                  </Menu>
              </menus>
            </MenuBar>
            <TabPane tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
              <tabs>
                <Tab text="Резюме">
                     <content>
                        <StackPane fx:id="backgroundImagePane">
                           <children>
                              <GridPane>
                                <columnConstraints>
                                    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="250.0" minWidth="250.0" prefWidth="250.0" />
                                  <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
                                  <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" />
                                  <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
                                    <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" maxWidth="200.0" minWidth="0.0" prefWidth="200.0" />
                                </columnConstraints>
                                <rowConstraints>
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                  <RowConstraints vgrow="NEVER" />
                                  <RowConstraints vgrow="NEVER" />
                                </rowConstraints>
                                 <children>
                                    <StackPane fx:id="tablePane" prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="4">
                                       <children>
                                          <TableView fx:id="table" prefWidth="200.0">
                                              <placeholder>
                                                  <Label text="" />
                                              </placeholder>
                                            <columns>
                                              <TableColumn fx:id="lastNameColumn" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="150.0" text="Фамилия" />
                                              <TableColumn fx:id="firstNameColumn" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="150.0" text="Имя" />
                                              <TableColumn fx:id="middleNameColumn" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="150.0" text="Отчество" />
                                                <TableColumn fx:id="professionColumn" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="100.0" text="Профессия" />
                                                <TableColumn fx:id="workTypeColumn" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="100.0" text="График" />
                                                <TableColumn fx:id="wageColumn" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="100.0" text="Зарплата" />
                                                <TableColumn fx:id="currencyColumn" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="100.0" text="Валюта" />
                                                <TableColumn fx:id="educationColumn" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="100.0" text="Образование" />
                                                <TableColumn fx:id="experienceColumn" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="100.0" text="Опыт работы" />
                                            </columns>
                                          </TableView>
                                       </children>
                                    </StackPane>
                                    <ScrollPane fitToWidth="true" hbarPolicy="NEVER" maxWidth="1.7976931348623157E308" style="-fx-background-color: white;" GridPane.rowSpan="3">
                                       <content>
                                          <VBox fx:id="profFieldVBox" />
                                       </content>
                                    </ScrollPane>
                                    <VBox fx:id="searchPane" spacing="5.0" style="-fx-background-color: white;" visible="false" GridPane.columnIndex="4" GridPane.rowSpan="3">
                                       <children>
                                          <TextField fx:id="professionTextField" promptText="Профессия" />
                                          <TextField fx:id="wageTextField" promptText="Зарплата" />
                                          <TextField fx:id="lastNameTextField" promptText="Фамилия" />
                                          <TextField fx:id="firstNameTextField" promptText="Имя" />
                                          <TextField fx:id="middleNameTextField" promptText="Отчество" />
                                          <ComboBox fx:id="educationComboBox" maxWidth="1.7976931348623157E308" promptText="Образование" />
                                          <ComboBox fx:id="workTypeComboBox" maxWidth="1.7976931348623157E308" promptText="График работы" />
                                          <AnchorPane>
                                             <children>
                                                <Button mnemonicParsing="false" onAction="#actionButtonFind" styleClass="grey" text="Поиск" AnchorPane.leftAnchor="0.0" />
                                                <Button layoutX="119.0" mnemonicParsing="false" onAction="#actionButtonClear" styleClass="grey" text="Очистить" AnchorPane.rightAnchor="0.0" />
                                             </children>
                                          </AnchorPane>
                                          <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#actionButtonExportToExel" styleClass="grey" text="Экспорт в exel">
                                             <VBox.margin>
                                                <Insets top="10.0" />
                                             </VBox.margin>
                                          </Button>
                                       </children>
                                       <padding>
                                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                       </padding>
                                    </VBox>
                                    <GridPane fx:id="buttonsPane" GridPane.columnIndex="1" GridPane.columnSpan="4" GridPane.rowIndex="1">
                                      <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" />
                                        <ColumnConstraints hgrow="ALWAYS" />
                                          <ColumnConstraints />
                                          <ColumnConstraints />
                                      </columnConstraints>
                                      <rowConstraints>
                                        <RowConstraints vgrow="SOMETIMES" />
                                        <RowConstraints vgrow="SOMETIMES" />
                                      </rowConstraints>
                                       <children>
                                          <Button mnemonicParsing="false" onAction="#actionButtonAdd" prefWidth="200.0" styleClass="black" text="Добавить" GridPane.columnIndex="3">
                                             <GridPane.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                             </GridPane.margin>
                                          </Button>
                                          <Button mnemonicParsing="false" onAction="#actionButtonOpen" prefWidth="200.0" styleClass="black" text="Открыть">
                                             <GridPane.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                             </GridPane.margin>
                                          </Button>
                                          <Button mnemonicParsing="false" onAction="#actionButtonEdit" prefWidth="200.0" styleClass="black" text="Редактировать" GridPane.rowIndex="1">
                                             <GridPane.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                             </GridPane.margin>
                                          </Button>
                                          <Button mnemonicParsing="false" onAction="#actionButtonDelete" prefWidth="200.0" styleClass="black" text="Удалить" GridPane.columnIndex="3" GridPane.rowIndex="1">
                                             <GridPane.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                             </GridPane.margin>
                                          </Button>
                                       </children>
                                    </GridPane>
                                 </children>
                              </GridPane>
                           </children>
                        </StackPane>
                     </content>
                </Tab>
                <Tab text="Вакансии">
                  <content>
                      <StackPane>
                           <children>
                              <VBox prefHeight="200.0" prefWidth="100.0">
                                 <children>
                                    <FlowPane prefWidth="200.0">
                                       <children>
                                          <Button mnemonicParsing="false" text="Добавить" />
                                          <Button mnemonicParsing="false" text="Редактировать" />
                                          <Button mnemonicParsing="false" text="Удалить" />
                                       </children>
                                    </FlowPane>
                                    <TableView prefHeight="200.0" prefWidth="200.0">
                                      <columns>
                                        <TableColumn prefWidth="75.0" text="C1" />
                                        <TableColumn prefWidth="75.0" text="C2" />
                                      </columns>
                                    </TableView>
                                    <GridPane>
                                      <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                      </columnConstraints>
                                      <rowConstraints>
                                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                      </rowConstraints>
                                       <children>
                                          <VBox spacing="5.0" GridPane.columnIndex="1">
                                             <children>
                                                <TextField promptText="Профессия" />
                                                <ComboBox maxWidth="1.7976931348623157E308" promptText="Проф. область" />
                                                <ComboBox maxWidth="1.7976931348623157E308" prefWidth="150.0" promptText="График работы" />
                                                <ComboBox maxWidth="1.7976931348623157E308" prefWidth="150.0" promptText="Образование" />
                                                <TextField promptText="Зарплата" />
                                                <ComboBox maxWidth="1.7976931348623157E308" prefWidth="150.0" promptText="Валюта" />
                                                <DatePicker maxWidth="1.7976931348623157E308" promptText="начало опыта раб." />
                                                <DatePicker maxWidth="1.7976931348623157E308" promptText="конец опыта раб." />
                                                <TextArea maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0" promptText="Адрес" />
                                             </children>
                                             <GridPane.margin>
                                                <Insets bottom="10.0" top="10.0" />
                                             </GridPane.margin>
                                          </VBox>
                                       </children>
                                    </GridPane>
                                 </children>
                                 <StackPane.margin>
                                    <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                                 </StackPane.margin>
                              </VBox>
                           </children></StackPane>
                  </content>
                </Tab>
              </tabs>
            </TabPane>
         </children>
      </VBox>
    </children>
</StackPane>
