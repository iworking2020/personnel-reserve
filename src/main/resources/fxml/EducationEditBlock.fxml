<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.*?>
<fx:root fx:id="parentEducation" spacing="10.0" type="VBox" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <AnchorPane>
         <children>
            <Button mnemonicParsing="false" onAction="#actionDelete" styleClass="grey" text="Удалить" AnchorPane.rightAnchor="0.0" />
              <ComboBox fx:id="educationComboBox" maxWidth="1.7976931348623157E308" promptText="Вид образования" AnchorPane.leftAnchor="0.0" />
         </children>
      </AnchorPane>
        <TextArea fx:id="editableText" prefHeight="100.0" promptText="Описание" />
    </children>
</fx:root>
