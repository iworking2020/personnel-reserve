<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox fx:id="companyEdit" spacing="5.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.iworking.personnel.reserve.controller.CompanyEditController">
    <children>
        <ComboBox fx:id="companyTypeComboBox" maxWidth="1.7976931348623157E308" promptText="Тип компании" />
        <TextField fx:id="nameCompanyTextField" promptText="Наименование" />
        <TextField fx:id="numberPhoneTextField" promptText="Номер тел." />
        <TextField fx:id="emailTextField" promptText="Эл. почта" />
        <TextField fx:id="webPageTextField" promptText="Сайт" />
        <TextArea fx:id="addressTextArea" maxHeight="-Infinity" prefHeight="25.0" promptText="Адрес" />
        <AnchorPane>
            <children>
                <Button mnemonicParsing="false" onAction="#actionCancel" styleClass="grey" text="Отмена" AnchorPane.leftAnchor="0.0">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                </Button>
            <TextField fx:id="companyIdTextField" editable="false" maxHeight="0.0" promptText="Ид" visible="false" />
                <Button fx:id="saveCompanyButton" mnemonicParsing="false" styleClass="grey" text="Сохранить" AnchorPane.rightAnchor="0.0">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                </Button>
            </children>
            <VBox.margin>
                <Insets top="5.0" />
            </VBox.margin>
        </AnchorPane>
    </children>
</VBox>
