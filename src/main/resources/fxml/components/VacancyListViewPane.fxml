<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<fx:root fx:id="parent" style="-fx-background-color: white;" type="javafx.scene.layout.BorderPane" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <FlowPane BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#actionBack" text="Назад">
               <FlowPane.margin>
                  <Insets bottom="5.0" left="5.0" />
               </FlowPane.margin>
               <styleClass>
                  <String fx:value="grey" />
                  <String fx:value="image-button" />
                  <String fx:value="image-back" />
               </styleClass></Button>
         </children>
         <padding>
            <Insets top="5.0" />
         </padding>
      </FlowPane>
   </top>
   <center>
      <ListView prefHeight="200.0" prefWidth="200.0" styleClass="grey" BorderPane.alignment="CENTER" />
   </center>
</fx:root>
