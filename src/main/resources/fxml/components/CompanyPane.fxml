<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root type="HBox" fx:id="parentPane" alignment="CENTER_LEFT" fillHeight="false" maxHeight="80.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fx:id="companyImageView" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
             <HBox.margin>
                <Insets bottom="5.0" right="10.0" top="5.0" />
             </HBox.margin></ImageView>
          <VBox>
              <children>
                  <Label fx:id="companyTypeLabel" text="ООО" />
                  <Label fx:id="companyNameLabel" text="&quot;Рога и копыта&quot;" />
              </children>
          </VBox>
    </children>
</fx:root>
