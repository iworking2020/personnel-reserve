<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<VBox maxWidth="225.0" spacing="5.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.iworking.personnel.reserve.controller.ResumesTreeController">
   <FlowPane>
      <children>
         <Button mnemonicParsing="false" styleClass="grey" text="Добавить">
            <FlowPane.margin>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </FlowPane.margin>
         </Button>
         <Button mnemonicParsing="false" onAction="#actionTreeUpdate" styleClass="grey" text="Обновить">
            <FlowPane.margin>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </FlowPane.margin></Button>
      </children>
   </FlowPane>
    <TreeView fx:id="resumesTreeView" />
</VBox>
